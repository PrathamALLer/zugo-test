{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "SET NODE_ENV=development && jest",
    "test:routes": "SET NODE_ENV=development && jest tests/routes",
    "dev": "SET NODE_ENV=development && nodemon server.js",
    "start": "SET NODE_ENV=production && node server.js",
    "pull-xano-db-schema": "node -r dotenv/config utils/xano/scripts/getDbSchema.js dotenv_config_path=.env.development",
    "db:create": "SET NODE_ENV=development && sequelize-cli db:create --config=config/db.js",
    "db:drop": "SET NODE_ENV=development && sequelize-cli db:drop --config=config/db.js",
    "migrate": "SET NODE_ENV=development && sequelize-cli db:migrate --config=config/db.js",
    "migrate:undo": "SET NODE_ENV=development && sequelize-cli db:migrate:undo --config=config/db.js",
    "migrate:undo:all": "SET NODE_ENV=development && sequelize-cli db:migrate:undo:all --config=config/db.js",
    "seed": "SET NODE_ENV=development && npx sequelize-cli db:seed:all --config=config/db.js",
    "db:reset": "SET NODE_ENV=development && sequelize-cli db:drop --config=config/db.js && sequelize-cli db:create --config=config/db.js && sequelize-cli db:migrate --config=config/db.js && npx sequelize-cli db:seed:all --config=config/db.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.7",
    "cli-progress": "^3.12.0",
    "dotenv": "^16.4.5",
    "joi": "^17.13.3",
    "nodemon": "^3.1.7",
    "pg": "^8.13.0",
    "sequelize": "^6.37.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "express": "^4.21.1",
    "jest": "^29.7.0",
    "sequelize-cli": "^6.6.2",
    "supertest": "^7.0.0"
  }
}
